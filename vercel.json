{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/fonts/VictorMono-Regular.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/Outfit-VariableFont_wght.ttf",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/IainSmith.pdf",
      "destination": "/robinson-resume-2019.pdf"
    },
    {
      "source": "/robinson-resume-2019.pdf",
      "destination": "https://read.cv/leerob"
    },
    {
      "source": "/resume",
      "destination": "https://read.cv/leerob"
    },
    {
      "source": "/projects",
      "destination": "/about"
    },
    {
      "source": "/blog/vercel-env-variables-size-limit",
      "destination": "https://vercel.com/support/articles/how-do-i-workaround-vercel-s-4-kb-environment-variables-limit"
    },
    {
      "source": "/30",
      "destination": "https://calendly.com/lee-vercel/30min"
    },
    {
      "source": "/15",
      "destination": "https://calendly.com/lee-vercel/15min"
    },
    {
      "source": "/youtube",
      "destination": "https://www.youtube.com/channel/UCZMli3czZnd1uoc1ShTouQw"
    },
    {
      "source": "/live",
      "destination": "https://www.youtube.com/channel/UCZMli3czZnd1uoc1ShTouQw/live"
    },
    {
      "source": "/linkedin",
      "destination": "https://www.linkedin.com/in/iainsmithmobile/"
    },
    {
      "source": "/stackoverflow",
      "destination": "https://stackoverflow.com/users/1107580/iain-smith"
    },
    {
      "source": "/github",
      "destination": "https://github.com/b099l3"
    },
    {
      "source": "/strava",
      "destination": "https://www.strava.com/athletes/23360470"
    }
  ]
}
